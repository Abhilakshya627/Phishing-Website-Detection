{
     "manifest_version": 2,
     "name": "Phishing Website Detector",
     "version": "1.0",
     "permissions": ["tabs", "<all_urls>"],
     "background": {
         "scripts": ["background.js"],
         "persistent": false
     },
     "content_scripts": [
         {
             "matches": ["<all_urls>"],
             "js": ["content.js"]
         }
     ],
     "browser_action": {
         "default_popup": "popup.html"
     }
 }